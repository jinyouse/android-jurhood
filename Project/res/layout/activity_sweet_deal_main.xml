<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_sweet_deals_640x1055"
    android:id="@+id/framelayout_container"
    tools:context=".AppStart" >

    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="60px"
        android:background="@drawable/bg_top_carousel_640x57"
        android:id="@+id/framelayout_topnavigation"
        android:tag="fw-sh" />
    
    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="100px"
        android:layout_gravity="bottom"
        android:id="@+id/framelayout_bottomtabbar"
        android:tag="fw-sh-gb" />
    
    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_marginTop="60px"
        android:layout_marginBottom="100px"
        android:tag="fw-fh-st-sb-glt"
        android:id="@+id/framelayout_content" />

</FrameLayout>
